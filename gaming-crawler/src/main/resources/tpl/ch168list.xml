<?xml version="1.0" encoding="UTF-8"?>
<page pageType="ch168list">
    <uielements objectName="beforeSemantic">
        <uiobject fieldName="tidyDom">
            <expression><![CDATA[$TIDYDOMFMT($CLEAN(html,$KEYVAL(task,"url")))]]></expression>
        </uiobject>
    </uielements>
    <uielements objectName="cr_sxb_car_car_sale">
        <rows><![CDATA[$ROWS(tidyDom,"//ul[@class='ch_carlistv3']/li/div/h3/a")]]></rows>
        <uiobject fieldName="id" nullAble="false">
            <expression><![CDATA[$UUID()]]></expression>
        </uiobject>
        <uiobject fieldName="car_number" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/div/h3/a/@href",row),"&id=(.*)")]]></expression>
        </uiobject>
        <uiobject fieldName="data_url" nullAble="false">
            <expression>
                <![CDATA[$XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/div/h3/a/@href",row)]]></expression>
        </uiobject>
        <uiobject fieldName="car_brand_name" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"brandName")]]></expression>
        </uiobject>
        <uiobject fieldName="car_series_name">
            <expression><![CDATA[$KEYVAL(task,"carSeriesName")]]></expression>
        </uiobject>
        <uiobject fieldName="car_type_name" nullAble="false">
            <expression>
                <![CDATA["20"+($MYCONTAINS($KEYVAL(task,"carTypeName"),"款")==true&&$MYCONTAINS($KEYVAL(task,"carTypeName"),"改款")==false?"":($REGEXP($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/div/h3/a",row),"(\\d{2}款)"))+" ")+$KEYVAL(task,"carTypeName")]]></expression>
        </uiobject>
        <uiobject fieldName="sale_price" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/div/span/b",row),"(\\d*[.]\\d*)")]]></expression>
        </uiobject>
        <uiobject fieldName="out_color" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($ARRAY($SPLIT($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c1']/text()",row),"\\|"),2),"(.*)/")]]></expression>
        </uiobject>
        <uiobject fieldName="interior_color" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($ARRAY($SPLIT($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c1']/text()",row),"\\|"),2),"/(.*)")]]></expression>
        </uiobject>
        <uiobject fieldName="sale_area" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c1']/text()",row),"销(.*)")==""?"全国":$REGEXP($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c1']/text()",row),"销(.*)")]]></expression>
        </uiobject>
        <uiobject fieldName="supply_day" nullAble="false">
            <expression>
                <![CDATA["4"]]></expression>
        </uiobject>
        <uiobject fieldName="seller_name" nullAble="false">
            <expression>
                <![CDATA[$XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c3']/a",row)]]></expression>
        </uiobject>
        <uiobject fieldName="car_source_address">
            <expression>
                <![CDATA[$REGEXP($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c3']/cite[@class='adr']",row),"车源所在地：(.*)")]]></expression>
        </uiobject>
        <uiobject fieldName="publish_time" nullAble="false">
            <expression>
                <![CDATA[$DATEFMT($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c3']/cite[1]",row),"HH:mm|MM-dd")]]></expression>
        </uiobject>
        <uiobject fieldName="source_type" nullAble="false">
            <expression>
                <![CDATA[$MYCONTAINS($XPATHCOLS(tidyDom,"//ul[@class='ch_carlistv3']/li/p[@class='c1']/text()",row),"现车")==true?"现货":"期货"]]></expression>
        </uiobject>
        <uiobject fieldName="created_time" nullAble="false">
            <expression><![CDATA[$SYSDATESTR()]]></expression>
        </uiobject>
        <uiobject fieldName="updated_time" nullAble="false">
            <expression><![CDATA[$SYSDATESTR()]]></expression>
        </uiobject>
        <uiobject fieldName="web_site" nullAble="false">
            <expression><![CDATA["ch168"]]></expression>
        </uiobject>
        <uiobject fieldName="md5" nullAble="false">
            <expression>
                <![CDATA[$MD5($KEYVAL(map,"web_site")+$KEYVAL(map,"car_number")+$KEYVAL(map,"sale_price")+$KEYVAL(map,"publish_time"))]]></expression>
        </uiobject>
        <uiobject fieldName="keyName" nullAble="false">
            <expression><![CDATA["car_number"]]></expression>
        </uiobject>
        <uiobject fieldName="keyValue" nullAble="false">
            <expression><![CDATA[$KEYVAL(map,"car_number")]]></expression>
        </uiobject>
    </uielements>
    <uielements objectName="nextTask">
        <rows><![CDATA[$XPATHROWS(tidyDom,"//a[(text()='下一页')]/@href")==""?0:1]]></rows>
        <uiobject fieldName="url" nullAble="false">
            <expression><![CDATA[$XPATHROWS(tidyDom,"//a[(text()='下一页')]/@href")]]></expression>
        </uiobject>
        <uiobject fieldName="templeName" nullAble="false">
            <expression><![CDATA["ch168list"]]></expression>
        </uiobject>
        <uiobject fieldName="brandName" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"brandName")]]></expression>
        </uiobject>
        <uiobject fieldName="carSeriesName" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"carSeriesName")]]></expression>
        </uiobject>
        <uiobject fieldName="carTypeName" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"carTypeName")]]></expression>
        </uiobject>
    </uielements>
</page>