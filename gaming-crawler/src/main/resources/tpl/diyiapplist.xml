<?xml version="1.0" encoding="gbk"?>
<page pageType="diyiapplist">
    <uielements objectName="beforeSemantic">
        <uiobject fieldName="tidyDom">
            <expression><![CDATA[$TIDYDOMFMT($CLEAN(html,$KEYVAL(task,"url")))]]></expression>
        </uiobject>
        <uiobject fieldName="currentPage" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"currentPage")+""==""?"1":$KEYVAL(task,"currentPage")+""]]></expression>
        </uiobject>
    </uielements>
    <uielements objectName="nextTask">
        <rows><![CDATA[$ROWS(tidyDom,"//div[@class='linet']/ul/li/div/a")]]></rows>
        <uiobject fieldName="url" nullAble="false">
            <expression><![CDATA[$XPATHCOLS(tidyDom,"//div[@class='linet']/ul/li/div/a/@href",row)]]></expression>
        </uiobject>
        <uiobject fieldName="titleImage" nullAble="false">
            <expression>
                <![CDATA[$XPATHCOLS(tidyDom,"//div[@class='linet']/ul/li/div/a[@class='fl']/img/@data-u",row)]]></expression>
        </uiobject>
        <uiobject fieldName="infoZoneType" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"infoZoneType")]]></expression>
        </uiobject>
        <uiobject fieldName="infoType" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"infoType")]]></expression>
        </uiobject>
        <uiobject fieldName="templeName" nullAble="false">
            <expression><![CDATA["diyiapparticle"]]></expression>
        </uiobject>
        <uiobject fieldName="lastUrl">
            <expression><![CDATA[$KEYVAL(task,"lastUrl")]]></expression>
        </uiobject>
        <uiobject fieldName="taskId">
            <expression><![CDATA[$KEYVAL(task,"id")]]></expression>
        </uiobject>
    </uielements>

    <uielements objectName="nextTask">
        <rows><![CDATA[$XPATHROWS(tidyDom,"//a[(text()='下一页')]/@href")==""?0:$ADD(currentPage+"",0+"")<3?1:0]]></rows>
        <uiobject fieldName="url" nullAble="false">
            <expression><![CDATA[$XPATHROWS(tidyDom,"//a[(text()='下一页')]/@href")]]></expression>
        </uiobject>
        <uiobject fieldName="templeName" nullAble="false">
            <expression><![CDATA["diyiapplist"]]></expression>
        </uiobject>
        <uiobject fieldName="infoZoneType" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"infoZoneType")]]></expression>
        </uiobject>
        <uiobject fieldName="infoType" nullAble="false">
            <expression><![CDATA[$KEYVAL(task,"infoType")]]></expression>
        </uiobject>
        <uiobject fieldName="lastUrl">
            <expression><![CDATA[$KEYVAL(task,"lastUrl")]]></expression>
        </uiobject>
        <uiobject fieldName="id">
            <expression><![CDATA[$KEYVAL(task,"id")]]></expression>
        </uiobject>
        <uiobject fieldName="currentPage">
            <expression><![CDATA[$ADD(currentPage+"",1+"")]]></expression>
        </uiobject>
    </uielements>

</page>