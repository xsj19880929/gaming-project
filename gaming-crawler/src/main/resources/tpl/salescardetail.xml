<?xml version="1.0" encoding="UTF-8"?>
<page pageType="salescardetail">
    <uielements objectName="beforeSemantic">
        <uiobject fieldName="tidyDom">
            <expression><![CDATA[$TIDYDOMFMT($CLEAN(html,$KEYVAL(task,"url")))]]></expression>
        </uiobject>
    </uielements>
    <uielements objectName="cr_sxb_car_car_sale">
        <uiobject fieldName="id" nullAble="false">
            <expression><![CDATA[$UUID()]]></expression>
        </uiobject>
        <uiobject fieldName="car_number" nullAble="false">
            <expression><![CDATA[$XPATHROWS(tidyDom,"//div[@class='detail-info-user']/a/@carnumber")]]></expression>
        </uiobject>
        <uiobject fieldName="car_brand_name" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//div[@class='detail-brand-infor']/p[1]"),"【(.*) ")]]></expression>
        </uiobject>
        <uiobject fieldName="car_series_name">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//div[@class='detail-brand-infor']/p[1]")," (.*)】")]]></expression>
        </uiobject>
        <uiobject fieldName="car_type_name" nullAble="false">
            <expression><![CDATA[$XPATHROWS(tidyDom,"//div[@class='detail-brand-infor']/p[2]")]]></expression>
        </uiobject>
        <uiobject fieldName="sale_price" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//p[@class='detail-car-price']"),"(\\d*[.]\\d*)")]]></expression>
        </uiobject>
        <uiobject fieldName="dis_count_price">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//p[@class='detail-car-price']"),"/[\\u4E00-\\u9FA5]{1}(\\d*[.]\\d*)")]]></expression>
        </uiobject>
        <uiobject fieldName="guide_price" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//ul[@class='detail-car-infor']/li[2]/p[1]"),"(.*)万元")]]></expression>
        </uiobject>
        <uiobject fieldName="dis_count_price_type">
            <expression>
                <![CDATA[$SUBSTR($REGEXP($XPATHROWS(tidyDom,"//p[@class='detail-car-price']"),"/(.*)"),0,1)]]></expression>
        </uiobject>
        <uiobject fieldName="dis_count_price_unit">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//p[@class='detail-car-price']"),"/[\\u4E00-\\u9FA5]{1}\\d*[.]\\d*(.*)")]]></expression>
        </uiobject>
        <uiobject fieldName="out_color" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//ul[@class='detail-car-infor']/li[3]/p[2]"),"(.*)/")]]></expression>
        </uiobject>
        <uiobject fieldName="interior_color" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//ul[@class='detail-car-infor']/li[3]/p[2]"),"/(.*)")]]></expression>
        </uiobject>
        <uiobject fieldName="sale_area" nullAble="false">
            <expression>
                <![CDATA[$XPATHROWS(tidyDom,"//ul[@class='detail-car-infor']/li[4]/p[2]")]]></expression>
        </uiobject>
        <uiobject fieldName="supply_day" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//ul[@class='detail-car-infor']/li[5]/p[2]"),"(.*)天之内")]]></expression>
        </uiobject>
        <uiobject fieldName="count" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//ul[@class='detail-car-infor']/li[6]/p[2]"),"(.*)辆")]]></expression>
        </uiobject>
        <uiobject fieldName="sale_car_sn" nullAble="false">
            <expression>
                <![CDATA[$XPATHROWS(tidyDom,"//ul[@class='detail-car-infor']/li[7]/p[2]")]]></expression>
        </uiobject>
        <uiobject fieldName="seller_name" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//span[@class='detail-user-name']"),"(.*)\\(")]]></expression>
        </uiobject>
        <uiobject fieldName="seller_phone" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//span[@class='detail-user-name']"),"\\((.*)\\)")]]></expression>
        </uiobject>
        <uiobject fieldName="publish_time" nullAble="false">
            <expression>
                <![CDATA[$DATEFMT($REGEXP($XPATHROWS(tidyDom,"//span[@class='detail-release-time']"),"发布时间:(.*)"),"yyyy-MM-dd HH:mm")]]></expression>
        </uiobject>
        <uiobject fieldName="source_type" nullAble="false">
            <expression>
                <![CDATA[$REGEXP($XPATHROWS(tidyDom,"//div[@class='detail-brand-infor']/p[1]"),"】(.*)")]]></expression>
        </uiobject>
        <uiobject fieldName="config">
            <expression>
                <![CDATA[$XPATHROWS(tidyDom,"//ul[@class='detail-car-remark']/li[1]/p")]]></expression>
        </uiobject>
        <uiobject fieldName="created_time" nullAble="false">
            <expression><![CDATA[$SYSDATESTR()]]></expression>
        </uiobject>
        <uiobject fieldName="updated_time" nullAble="false">
            <expression><![CDATA[$SYSDATESTR()]]></expression>
        </uiobject>
        <uiobject fieldName="web_site" nullAble="false">
            <expression><![CDATA["sxb"]]></expression>
        </uiobject>
        <uiobject fieldName="keyName" nullAble="false">
            <expression><![CDATA["car_number"]]></expression>
        </uiobject>
        <uiobject fieldName="keyValue" nullAble="false">
            <expression><![CDATA[$XPATHROWS(tidyDom,"//div[@class='detail-info-user']/a/@carnumber")]]></expression>
        </uiobject>
    </uielements>
</page>